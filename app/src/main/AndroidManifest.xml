<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hamza.flythedrogo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/new_fg_2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name="com.example.hamza.flythedrogo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.hamza.flythedrogo.gameStart"></activity>
        <activity android:name="com.example.hamza.flythedrogo.highScoreDB"></activity>
    </application>

</manifest>